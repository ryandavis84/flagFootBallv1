<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity EmergencyContact.
    -->
    <changeSet id="20201229212625-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="player_id"
                                 baseTableName="emergency_contact"
                                 constraintName="fk_emergency_contact_player_id"
                                 referencedColumnNames="id"
                                 referencedTableName="player"/>

        <addForeignKeyConstraint baseColumnNames="coach_id"
                                 baseTableName="emergency_contact"
                                 constraintName="fk_emergency_contact_coach_id"
                                 referencedColumnNames="id"
                                 referencedTableName="coach"/>

    </changeSet>
</databaseChangeLog>
