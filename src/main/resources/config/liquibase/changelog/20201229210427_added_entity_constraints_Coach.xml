<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Coach.
    -->
    <changeSet id="20201229210427-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="team_id"
                                 baseTableName="coach"
                                 constraintName="fk_coach_team_id"
                                 referencedColumnNames="id"
                                 referencedTableName="team"/>

        <addForeignKeyConstraint baseColumnNames="contact_info_id"
                                 baseTableName="coach"
                                 constraintName="fk_coach_contact_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="contact_info"/>

    </changeSet>
</databaseChangeLog>
